  "scripts": {
    "build": "nest build",
    "dev": "nest start --watch --preserveWatchOutput",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug",
    "start:prod": "node dist/main",
    "lint": "yarn lint:backend",
    "lint:fix": "yarn lint:backend --write",
    "test:runner": "node --expose-gc --no-compilation-cache ../../node_modules/jest-cli/bin/jest.js --config test/jest.config.ts",
    "test": "yarn run test:runner --runInBand",
    "test:auth": "yarn run test --testNamePattern='^AuthController'",
    "test:cov": "yarn run test --coverage",
    "test:ci": "yarn run test:cov --bail=1",
    "test:watch": "yarn run test",
    "test:debug": "yarn run test --inspect-brk",
    "generator": "ts-node generator",
    "g": "yarn generator",
    "typeorm-extension": "dotenv -- ts-node ../../node_modules/typeorm-extension/bin/cli.cjs -d src/integrations/typeorm/typeorm-datasource.config.ts",
    "db:drop": "yarn typeorm-extension db:drop",
    "db:create": "yarn typeorm-extension db:create",
    "db:reset": "yarn db:drop && yarn db:create && yarn migration:up && yarn migration:seed",
    "migration:up": "dotenv -- typeorm-ts-node-commonjs migration:run -d src/integrations/typeorm/typeorm-datasource.config.ts",
    "migration:seed": "dotenv -- typeorm-ts-node-commonjs migration:run -d src/integrations/typeorm/typeorm-datasource-seed.config.ts",
    "docker:migration:up": "yarn typeorm migration:run -d dist/src/integrations/typeorm/typeorm-datasource.config.js",
    "docker:migration:seed": "yarn typeorm migration:run -d dist/src/integrations/typeorm/typeorm-datasource-seed.config.js"
  },
