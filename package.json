  "scripts": {
    "build": "nest build",
    "dev": "nest start --watch --preserveWatchOutput",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug",
    "start:prod": "node dist/main",
    "lint": "pnpm run lint:backend",
    "lint:fix": "pnpm run lint:backend --write",
    "test:runner": "node --expose-gc --no-compilation-cache ../../node_modules/jest-cli/bin/jest.js --config test/jest.config.ts",
    "test": "pnpm run test:runner --runInBand",
    "test:cov": "pnpm run test --coverage",
    "test:ci": "pnpm run test:cov --bail=1",
    "test:watch": "pnpm run test",
    "test:debug": "pnpm run test --inspect-brk",
    "generator": "ts-node generator",
    "g": "pnpm run generator",
    "typeorm-extension": "dotenv -- ts-node ../../node_modules/typeorm-extension/bin/cli.cjs -d src/integrations/typeorm/typeorm-datasource.config.ts",
    "db:drop": "pnpm typeorm-extension db:drop",
    "db:create": "pnpm typeorm-extension db:create",
    "db:reset": "pnpm db:drop && pnpm db:create && pnpm migration:up && pnpm migration:seed",
    "migration:up": "dotenv -- typeorm-ts-node-commonjs migration:run -d src/integrations/typeorm/typeorm-datasource.config.ts",
    "migration:seed": "dotenv -- typeorm-ts-node-commonjs migration:run -d src/integrations/typeorm/typeorm-datasource-seed.config.ts",
    "docker:migration:up": "pnpm typeorm migration:run -d dist/src/integrations/typeorm/typeorm-datasource.config.js",
    "docker:migration:seed": "pnpm typeorm migration:run -d dist/src/integrations/typeorm/typeorm-datasource-seed.config.js"
  },
